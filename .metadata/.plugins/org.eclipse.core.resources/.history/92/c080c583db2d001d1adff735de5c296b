package staticex;

public class Student {
	static int serialNum = 10000; //static변수
	
	int studentId;	//인스턴스변수
	String studentName;
	
	public Student() {	//생성자호출
		serialNum++;
		studentId = serialNum;
	}

	public static int getSerialNum() {
		int i = 10;
		i++;
		System.out.println(i);
		
		studentName = "홍길동";
		
		return serialNum;
	}
	
	
	
	
}
